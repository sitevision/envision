---
const { icons, sprite } = Astro.props as {
   icons: string[];
   sprite?: string;
};
---

<ul class="env-list env-list--horizontal--fixed env-list--horizontal--fixed--6">
   {
      icons.map((icon) => (
         <li class="env-list__item" title={icon}>
            <svg class="env-icon env-icon--small">
               <use href={`/sitevision/${sprite}#${icon}`} />
            </svg>
         </li>
      ))
   }
</ul>
